<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  
  <Import Project="../LINVAST.targets" />
  
  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <Authors>Ivan Ristović</Authors>
    <Description>Language-invariant AST library with extendible and intuitive interfaces.</Description>
    <Copyright>Ivan Ristović</Copyright>
    <RepositoryUrl>https://github.com/LINVAST/LINVAST</RepositoryUrl>
    <PackageProjectUrl>https://github.com/LINVAST/LINVAST</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Logging\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Antlr4.Runtime.Standard" Version="4.9.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="Serilog" Version="2.10.0" />
    <PackageReference Include="System.Collections.Immutable" Version="5.0.0" />
    <PackageReference Include="System.Linq" Version="4.3.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
